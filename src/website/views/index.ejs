<!DOCTYPE html>
<html>
  <head>
    <title><%= client.user.username %> - Homepage</title>
    <link rel="stylesheet" href="styles.css">
  </head>
  <body>
    <div class="stats-box">
      <div class="servers">
        <% setInterval(function() { %>
        <span><%= client.guilds.cache.size %></span>
        <% }, 1000) %>
      </div>
      <div class="channels">
        <% setInterval(function() { %>
        <span><%= client.channels.cache.size %></span>
        <% }, 1000) %>
      </div>
      <div class="users">
        <span></span>
      </div>
    </div>
  </body>
</html>